<h1>Sessions#new</h1>

<%= form_tag do %>
  <div>
    <%= label_tag :email, 'Email:' %>
    <%= text_field_tag :email, params[:email] %>
  </div>
  <div>
    <%= label_tag :password, 'Password:' %>
    <%= text_field_tag :password, params[:password] %>
  </div>
  <div>
    <%= submit_tag 'Login' %>
  </div>
  <% end %>
